{
	"version": "2",
	"templates": [
		{
			"type": 1,
			"title": "Registry",
			"description": "Docker image registry",
			"categories": [
				"docker"
			],
			"platform": "linux",
			"logo": "https://raw.githubusercontent.com/edwinpang/portainer_image/master/image/registry.png",
			"image": "registry:latest",
			"ports": [
				"5000/tcp"
			],
			"volumes": [{
				"container": "/var/lib/registry"
			}]
		},
		{
			"type": 1,
			"title": "Nginx",
			"description": "High performance web server",
			"categories": [
				"webserver"
			],
			"platform": "linux",
			"logo": "https://raw.githubusercontent.com/edwinpang/portainer_image/master/image/nginx.png",
			"image": "nginx:latest",
			"ports": [
				"80/tcp",
				"443/tcp"
			],
			"volumes": [{
					"container": "/etc/nginx"
				},
				{
					"container": "/usr/share/nginx/html"
				}
			]
		},
		{
			"type": 1,
			"title": "Nginx-PHP73-FPM",
			"description": "High performance web server",
			"categories": [
				"webserver"
			],
			"platform": "linux",
			"logo": "https://raw.githubusercontent.com/edwinpang/portainer_image/master/image/nginx.png",
			"image": "wyveo/nginx-php-fpm:php73",
			"ports": [
				"80/tcp",
				"443/tcp"
			],
			"volumes": [{
					"container": "/etc/nginx"
				},
				{
					"container": "/usr/share/nginx/html"
				}
			]
		},
		{
			"type": 1,
			"title": "Httpd",
			"description": "Open-source HTTP server",
			"categories": [
				"webserver"
			],
			"platform": "linux",
			"logo": "https://raw.githubusercontent.com/edwinpang/portainer_image/master/image/httpd.png",
			"image": "httpd:latest",
			"ports": [
				"80/tcp"
			],
			"volumes": [{
				"container": "/usr/local/apache2/htdocs/"
			}]
		},
		{
			"type": 1,
			"title": "MySQL",
			"description": "The most popular open-source database",
			"categories": [
				"database"
			],
			"platform": "linux",
			"logo": "https://raw.githubusercontent.com/edwinpang/portainer_image/master/image/mysql.png",
			"image": "jypsolution/mysql-8.0.22",
			"env": [{
					"name": "MYSQL_ROOT_PASSWORD",
					"label": "Root password"
				},
				{
					"name": "TZ",
					"label": "Timezone",
					"default": "Asia/Kuala_Lumpur"
				}
			],
			"ports": [
				"3306/tcp"
			],
			"volumes": [{
				"container": "/var/lib/mysql"
			}]
		},
		{
			"type": 1,
			"title": "MariaDB",
			"description": "Performance beyond MySQL",
			"categories": [
				"database"
			],
			"platform": "linux",
			"logo": "https://raw.githubusercontent.com/edwinpang/portainer_image/master/image/mariadb.png",
			"image": "mariadb:latest",
			"env": [{
					"name": "MYSQL_ROOT_PASSWORD",
					"label": "Root password"
				},
				{
					"name": "TZ",
					"label": "Timezone",
					"default": "Asia/Kuala_Lumpur"
				}
			],
			"ports": [
				"3306/tcp"
			],
			"volumes": [{
				"container": "/var/lib/mysql"
			}]
		},
		{
			"type": 1,
			"title": "PostgreSQL",
			"description": "The most advanced open-source database",
			"categories": [
				"database"
			],
			"platform": "linux",
			"logo": "https://raw.githubusercontent.com/edwinpang/portainer_image/master/image/postgres.png",
			"image": "postgres:latest",
			"env": [{
					"name": "POSTGRES_USER",
					"label": "Superuser"
				},
				{
					"name": "POSTGRES_PASSWORD",
					"label": "Superuser password"
				}
			],
			"ports": [
				"5432/tcp"
			],
			"volumes": [{
				"container": "/var/lib/postgresql/data"
			}]
		},
		{
			"type": 1,
			"title": "Mongo",
			"description": "Open-source document-oriented database",
			"categories": [
				"database"
			],
			"platform": "linux",
			"logo": "https://raw.githubusercontent.com/edwinpang/portainer_image/master/image/mongo.png",
			"image": "mongo:latest",
			"ports": [
				"27017/tcp"
			],
			"volumes": [{
				"container": "/data/db"
			}]
		},
		{
			"type": 1,
			"title": "tiredofit/mongodb-backup",
			"description": "tiredofit/mongodb-backup",
			"categories": [
				"database"
			],
			"platform": "linux",
			"logo": "https://raw.githubusercontent.com/edwinpang/portainer_image/master/image/mongo.png",
			"image": "tiredofit/mongodb-backup:latest",
			"volumes": [{
				"container": "/backups"
			}],
			"env": [
				{
					"label": "DB_HOST",
					"name": "DB_HOST",
					"description":"Server Hostname e.g localhost/ip"
				},
				{
					"label": "DB_PORT",
					"name": "DB_PORT",
					"description":"mongo default port: 27017"
				},
				{
					"label": "DB_USER",
					"name": "DB_USER",
					"description":"DB Username"
				},
				{
					"label": "DB_PASS",
					"name": "DB_PASS",
					"description":"DB Password"
				},
				{
					"label": "DB_DUMP_FREQ",
					"name": "DB_DUMP_FREQ",
					"description":"How often to do a dump, in minutes. Defaults to 1440 minutes, or once per day."
				},
				{
					"label": "DB_CLEANUP_TIME",
					"name": "DB_CLEANUP_TIME",
					"default":"1440",
					"description":"How often to do a clean up, in minutes. Defaults to 1440 minutes, or once per day."
				}
			]
		},
		{
			"type": 1,
			"title": "Jenkins",
			"description": "Open-source continuous integration tool",
			"categories": [
				"continuous-integration"
			],
			"platform": "linux",
			"logo": "https://raw.githubusercontent.com/edwinpang/portainer_image/master/image/jenkins.png",
			"image": "jenkins/jenkins:lts",
			"ports": [
				"8080/tcp",
				"50000/tcp"
			],
			"env": [{
				"name": "JENKINS_OPTS",
				"label": "Jenkins options"
			}],
			"volumes": [{
				"container": "/var/jenkins_home"
			}]
		},
		{
			"type": 1,
			"title": "Jenkins-2.249.1",
			"description": "Open-source continuous integration tool",
			"categories": [
				"continuous-integration"
			],
			"platform": "linux",
			"logo": "https://raw.githubusercontent.com/edwinpang/portainer_image/master/image/jenkins.png",
			"image": "jenkins/jenkins:2.249.1-lts",
			"ports": [
				"8080/tcp",
				"50000/tcp"
			],
			"env": [{
				"name": "JENKINS_OPTS",
				"label": "Jenkins options"
			}],
			"volumes": [{
				"container": "/var/jenkins_home"
			}]
		},
		{
			"type": 1,
			"title": "Jenkins-2.277.1",
			"description": "Open-source continuous integration tool",
			"categories": [
				"continuous-integration"
			],
			"platform": "linux",
			"logo": "https://raw.githubusercontent.com/edwinpang/portainer_image/master/image/jenkins.png",
			"image": "jenkins/jenkins:2.277.1-lts",
			"ports": [
				"8080/tcp",
				"50000/tcp"
			],
			"env": [{
				"name": "JENKINS_OPTS",
				"label": "Jenkins options"
			}],
